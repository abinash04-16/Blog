     password: '',\n                invalidInput: null,\n            };\n        },\n        methods:{\n            submit(){\n                if(this.mail === \"\" || this.password === \"\" || this.password.length < 6 || !this.mail.includes('@'))\n                {\n                    this.invalidInput = true;\n                }\n                else\n                {\n                    this.$store.dispatch('login',{\n                        mail: this.mail,\n                        password: this.password,\n                    })                    \n                }\n            },\n            backClick()\n            {\n                this.invalidInput = false;\n            }\n        }\n    }\n\n</script>\n\n<style scoped>\n.container\n{\n    width: 25%;\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%,-50%);\n    color: white;\n}\n.container h2\n{\n    float: left;\n    font-size: 37px;\n    margin-bottom: 30px 0;\n    padding-bottom: 20px;\n    border-bottom: 3px solid rgb(81, 255, 0);\n}\n\n.textbox\n{\n    width: 100%;\n    padding: 10px;\n    overflow: hidden;\n    margin: 25px 0;\n    border-bottom: 1px solid #333;\n}\n\n.textbox input{\n    border: none;\n    outline: none;\n    font-size: 18px;\n    background: none;\n    color: #000;\n}\n\n.btn button\n{\n    background: none;\n    outline: none;\n    border: 2px solid #333;\n    padding: 10px;\n    color: white;\n    width: 100%;\n    text-transform: uppercase;\n    font-size: 20px;\n    cursor: pointer;\n    margin: 12px;\n}\n.altLink\n{\n    margin: 20px 0;\n    text-align: center;\n    font-size: 18px;\n}\n.altLink p\n{\n    color: black;\n}\n\n.altLink a\n{\n    color: white;\n    text-decoration: none;\n    cursor: pointer;\n}\n</style>"]}]}